                 <div class="dealpad">
                  <div class="row">
                    <div class="col-md-6">
                      <%= klass.place %><br>
                      <%= klass.date %>
                    </div>  
                     <div class="col-md-6">

                     <%= button_to "From: #{klass.price}â‚ª", klass, :method => "get", class: 'button' %>
              Session
                  </div>
                </div>
              </div>
          </div>
      </li>
    <% end %>
    <% end %>
</div>
</div>
